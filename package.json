{
  "name": "ReactNativeBoilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e-build-ios": "detox build --configuration ios",
    "test:e2e-run-ios": "detox test --configuration ios",
    "test:e2e-build-android": "detox build --configuration android",
    "test:e2e-run-android": "detox test --configuration android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.0.1",
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/checkbox": "^0.5.6",
    "@react-native-community/google-signin": "^5.0.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.2.0",
    "@react-navigation/bottom-tabs": "^5.10.4",
    "@react-navigation/native": "^5.8.4",
    "@react-navigation/stack": "^5.12.1",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/react-redux": "^7.1.11",
    "axios": "^0.21.0",
    "i18next": "^19.8.4",
    "lodash": "^4.17.20",
    "react": "16.13.1",
    "react-hook-form": "^6.12.0",
    "react-i18next": "^11.7.3",
    "react-native": "0.63.3",
    "react-native-appearance": "^0.3.4",
    "react-native-config": "^1.4.1",
    "react-native-fbsdk": "^3.0.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-get-random-values": "^1.5.1",
    "react-native-inappbrowser-reborn": "^3.5.1",
    "react-native-localize": "^2.0.1",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.12.0",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.2.2",
    "redux-persist": "^6.0.0",
    "uuid": "^8.3.2",
    "yup": "^0.31.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/react-native": "^0.63.2",
    "@types/react-native-fbsdk": "^1.1.1",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^26.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "detox": "^18.7.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^4.3.0",
    "jest": "^26.0.0",
    "jest-circus": "^26.0.0",
    "lint-staged": "^10.5.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.1.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run ts-compile-check && yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
