{
  "name": "react-native-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:e2e-build-ios": "detox build --configuration ios",
    "test:e2e-run-ios": "detox test --configuration ios",
    "test:e2e-build-android": "detox build --configuration android",
    "test:e2e-run-android": "detox test --configuration android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit --skipLibCheck",
    "rename": "sh ./scripts/rename.sh",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "2.6.0",
    "@invertase/react-native-apple-authentication": "2.1.2",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/checkbox": "0.5.8",
    "@react-native-community/google-signin": "5.0.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-firebase/app": "12.1.0",
    "@react-navigation/bottom-tabs": "5.11.11",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "@reduxjs/toolkit": "1.6.0",
    "@types/react-redux": "7.1.16",
    "axios": "0.21.1",
    "color": "3.1.3",
    "i18next": "20.3.2",
    "is-ci": "3.0.0",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-hook-form": "7.10.1",
    "react-i18next": "11.11.1",
    "react-native": "0.64.2",
    "react-native-config": "1.4.2",
    "react-native-fbsdk": "3.0.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-get-random-values": "1.7.0",
    "react-native-image-crop-picker": "0.36.2",
    "react-native-inappbrowser-reborn": "3.6.3",
    "react-native-localize": "2.1.1",
    "react-native-reanimated": "2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "8.1.0",
    "react-redux": "7.2.4",
    "redux-persist": "6.0.0",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/runtime": "7.14.6",
    "@react-native-community/eslint-config": "3.0.0",
    "@types/color": "3.0.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/react-native": "0.64.10",
    "@types/react-native-fbsdk": "3.0.0",
    "@types/react-native-vector-icons": "6.4.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/uuid": "8.3.1",
    "@types/yup": "0.29.12",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-jest": "27.0.6",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "18.18.1",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-native": "3.11.0",
    "husky": "6.0.0",
    "jest": "27.0.6",
    "jest-circus": "27.0.6",
    "lint-staged": "11.0.0",
    "metro-react-native-babel-preset": "0.66.1",
    "prettier": "2.3.2",
    "react-native-clean-project": "3.6.4",
    "react-native-svg-transformer": "0.14.3",
    "react-test-renderer": "17.0.2",
    "typescript": "4.3.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/__mocks__/svgMock.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run ts-compile-check && yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
